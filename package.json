  "contributors": [
    "Warix",
    "Ota"
  ],
  "node": ">=16.6.0",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "asciiart-logo": "^0.2.7",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^22.9.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
